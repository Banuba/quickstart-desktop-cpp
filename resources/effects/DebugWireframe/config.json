{
    "assets": {
        "attachments": {
            "effect_color": {
                "type": "color"
            },
            "effect_depth": {
                "clear": [
                    1.0
                ],
                "type": "depth"
            }
        },
        "images": {
            "camera: tex_y": {
                "type" : "camera",
                "format": "y"
            },
            "camera: tex_uv": {
                "type" : "camera",
                "format": "uv"
            },
            "camera: tex_rgb": {
                "type" : "camera",
                "format": "rgba"
            }
        },
        "materials": {
            "$builtin$materials/camera": {
                "samplers": [
                    {
                        "name": "tex_y",
                        "image": "camera: tex_y"
                    },
                    {
                        "name": "tex_uv",
                        "image": "camera: tex_uv"
                    },
                    {
                        "name": "tex_rgb",
                        "image": "camera: tex_rgb"
                    }
                ],
                "state": {
                    "backfaces": true,
                    "blend": "off",
                    "colorwrite": true,
                    "ztest": false,
                    "zwrite": false
                },
                "parameters": {
                    "bnb_camera_orientation": {
                        "type": "vec4",
                        "value": [1, 0, 0, 1]
                    },

                    "bnb_camera_scale": {
                        "type": "vec4",
                        "value": [1, 0, 0, 1]
                    },

                    "bnb_view_width_fraction": {
                        "type": "vec4",
                        "value": [1, 0, 0, 0]
                    }
                }
            },
            "wireframe": {
                "samplers": [
                ],
                "state": {
                    "backfaces": false,
                    "blend": "off",
                    "colorwrite": true,
                    "ztest": false,
                    "zwrite": false
                },
                "geometry_topology": "lines_list",
                "parameters": {}
            }
        },
        "meshes": {
            "!bnb_FACE": "$builtin$meshes/wireframe.stream:0",
            "!bnb_FACE1": "$builtin$meshes/wireframe.stream:1",
            "camera_surface": "$builtin$meshes/fs_tri"
        },
        "faces": {
            "face0": {
                "index": 0,
                "mesh": "!bnb_FACE"
            },
            "face1": {
                "index": 1,
                "mesh": "!bnb_FACE1"
            }
        }
    },
    "components": {
        "camera_tri": {
            "materials": ["$builtin$materials/camera"],
            "mesh": "camera_surface",
            "type": "mesh_instance"
        },
        "face:0": {
            "materials": ["wireframe"],
            "mesh": "!bnb_FACE",
            "type": "mesh_instance"
        },
        "face:1": {
            "materials": ["wireframe"],
            "mesh": "!bnb_FACE1",
            "type": "mesh_instance"
        },
        "face_tracker:0": {
            "face": "face0",
            "type": "face_tracker"
        },
        "face_tracker:1": {
            "face": "face1",
            "type": "face_tracker"
        }
    },
    "entities": {
        "camera": {
            "components": [
                "camera_tri"
            ],
            "layer": "composer_pre"
        },
        "face0": {
            "components": [
                "face:0"
            ],
            "layer": "retouch"
        },
        "face1": {
            "components": [
                "face:1"
            ],
            "layer": "retouch"
        },
        "face_tracker0": {
            "components": [
                "face_tracker:0"
            ]
        },
        "face_tracker1": {
            "components": [
                "face_tracker:1"
            ]
        }
    },
    "hierarchy": {
        "face_tracker0": [
            "face0"
        ],
        "face_tracker1": [
            "face1"
        ],
        "root": [
            "camera",
            "face_tracker0",
            "face_tracker1"
        ]
    },
    "layers": {
        "composer_pre": {},
        "camera_copy": {},
        "retouch": {},
        "morph": {}
    },
    "render_list": {
        "default": [
            {
                "layer": "composer_pre",
                "render_target": "EffectRT"
            },
            {
                "layer": "retouch",
                "render_target": "EffectRT"
            }
        ]
    },
    "render_targets": {
        "EffectRT": [
            "effect_color",
            "effect_depth"
        ]
    },


    "scene": "face_tracker"
}